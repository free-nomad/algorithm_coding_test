   "source": [
    "# 코드 작성\n",
    "\n",
    "num = int(input())\n",
    "score = list(map(int, input().split()))\n",
    "max_num = max(score)\n",
    "sum = 0\n",
    "\n",
    "for i in range(num):\n",
    "    sum += (int(score[i]) / max_num * 100)\n",
    "    \n",
    "print(sum / num)"
